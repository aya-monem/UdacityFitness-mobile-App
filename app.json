{
  "expo": {
    "orientation": "portrait",
    "name": "udacityFitness",
    "description": "The best triathlon tracker ever",
    "slug": "udacityFitness",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "icon": "https://tylermcginnis.com/udacifitness-512.png",
    "notification": {
      "icon": "https://tylermcginnis.com/udacifitness-45.png"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.udacifitness.exp"
    },

    "android" : {
      "package": "org.udacifitness.exp"
    },
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ]
    
  }
}
